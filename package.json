{
  "name": "onboard-engine",
  "version": "1.5.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./index.css": "./dist/index.css",
    "./dist/index.css": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit",
    "lint:eslint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "prepublishOnly": "npm run build"
  },
  "private": false,
  "description": "A lightweight, powerful onboarding and product tour engine for React applications.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Forsrobin/onboard-engine.git"
  },
  "keywords": [
    "react",
    "onboarding",
    "product-tour",
    "walkthrough",
    "guide",
    "user-onboarding",
    "ui",
    "ux",
    "onboard-engine",
    "nextjs"
  ],
  "author": "Robin Fors",
  "license": "MIT",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/Forsrobin/onboard-engine/issues"
  },
  "homepage": "https://github.com/Forsrobin/onboard-engine#readme",
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.10.6",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^26.0.0",
    "prettier": "^3.8.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "vitest": "^4.0.17"
  },
  "dependencies": {
    "js-cookie": "^3.0.5"
  }
}
